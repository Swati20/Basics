/*http://www.spoj.com/problems/STRHH/*/
#include <stdio.h>
#include <string.h>
int main(void)
{
    char name[200];
    int n,i,j;
    scanf("%d",&n);
    for(i=0;i<n;i++) {
        scanf("%s",name);
        fflush(stdout);
        for(j=0;j<strlen(name)/2;j+=2) {
            printf("%c",name[j]);
        }
        printf("\n");
    }
    return 0;
}
